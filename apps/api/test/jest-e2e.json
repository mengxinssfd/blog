{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@/(.*?)$": "<rootDir>/../src/$1",
    "^@config/(.*?)$": "<rootDir>/../config/$1",
    "^@blog/(.*?)$": "<rootDir>/../../../packages/$1/src"
  }
}
